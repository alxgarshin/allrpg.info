includes:
	- phpstan-baseline.neon
parameters:
	dynamicConstantNames:
		- KIND
		- CMSVC
		- OBJ_TYPE
		- OBJ_ID
		- BID
		- MODAL
		- TEST_MODE
		- REQUEST_PROJECT_ID
	bootstrapFiles:
		- src/bootstrap.php
	level: 5
	paths:
